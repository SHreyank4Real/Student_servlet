  		<url-pattern>/dis_student</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  	<servlet-name>DisplayFaculty</servlet-name>
  	<servlet-class>serv.DisplayFaculty</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
