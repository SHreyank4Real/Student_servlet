  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>AddMentor</servlet-name>
  		<url-pattern>/add_mentor</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  	<servlet-name>RemoveStudent</servlet-name>
  	<servlet-class>serv.RemoveStudent</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>RemoveStudent</servlet-name>
  		<url-pattern>/rem_student</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  	<servlet-name>RemoveFaculty</servlet-name>
  	<servlet-class>serv.RemoveFaculty</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>RemoveFaculty</servlet-name>
  		<url-pattern>/rem_faculty</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  	<servlet-name>RemoveMentor</servlet-name>
  	<servlet-class>serv.RemoveMentor</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>RemoveMentor</servlet-name>
  		<url-pattern>/rem_mentor</url-pattern>
  	</servlet-mapping>
  	
