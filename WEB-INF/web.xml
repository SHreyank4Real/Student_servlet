<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletPractice</display-name>
  	<servlet>
  	<servlet-name>AddStudent</servlet-name>
  	<servlet-class>serv.AddStudent</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>AddStudent</servlet-name>
  		<url-pattern>/add_student</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  	<servlet-name>AddFaculty</servlet-name>
  	<servlet-class>serv.AddFaculty</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>AddFaculty</servlet-name>
  		<url-pattern>/add_faculty</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
